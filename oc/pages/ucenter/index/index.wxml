<view class="container">
  <view class="userInfo">
    <view class="avatar">
      <t-avatar shape="round" image="{{userInfo.avatarUrl}}" size="large"/>
    </view>
    <view class="name">{{userInfo.nickName}}</view>
  </view>

  <van-divider />

  <view wx:for="{{menuData}}" wx:key="item" class="cell-box">
    <t-cell-group>
      <t-cell
        wx:for="{{item}}"
        wx:for-item="xitem"
        wx:for-index="xindex"
        wx:key="xindex"
        title="{{xitem.title}}"
        arrow="{{!xitem.icon}}"
        note="{{xitem.tit}}"
        data-type="{{xitem.type}}"
        bordered="{{false}}"
        bind:click="onClickCell"
        t-class="t-cell-padding"
        t-class-note="order-group-note"
        t-class-left="order-group__left"
      >
        <t-icon name="{{xitem.icon}}" size="48rpx" slot="note" />
      </t-cell>
    </t-cell-group>
  </view>

