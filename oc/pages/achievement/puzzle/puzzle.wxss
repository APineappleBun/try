.container {
  padding: 0px;
  display: flex;
  flex-direction: column;
  justify-content:flex-start;
}
.item {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;  
}
.themeName {
  width: 100%;
  font-size: 28px;
  background-color: rgba(255,255,255,0.8);
}
.theme {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
}
.themeImage {
  width: 250px;
}
.puzzleList {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 25%;
  margin: 0 4%;
}
.puzzleItem {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 100%;
}
.isCollected {
  width: 100%;
}

.isNotCollected {
  filter: grayscale(100%); 
  width: 100%;
}